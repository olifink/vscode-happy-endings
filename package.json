{
  "name": "vscode-happy-endings",
  "displayName": "vscode-happy-endings",
  "description": "",
  "publisher": "olifink",
  "version": "0.0.1",
  "icon": "train.png",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "url": "https://github.com/olifink/vscode-happy-endings"
  },
  "license": "MIT License",
  "bugs": {
    "url": "https://github.com/olifink/vscode-happy-endings/issues"
  },
  "activationEvents": [
    "onCommand:vscode-happy-endings.endOfLine",
    "onCommand:vscode-happy-endings.comment"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-happy-endings.endOfLine",
        "title": "Happy: Smart semicolon at end of line end"
      },
      {
        "command": "vscode-happy-endings.comment",
        "title": "Happy: Smart comment current line and move to next"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-happy-endings.endOfLine",
        "key": "ctrl+alt+enter",
        "mac": "cmd+alt+enter",
        "when": "editorTextFocus"
      },
      {
        "command": "vscode-happy-endings.comment",
        "key": "ctrl+shift+/",
        "mac": "cmd+shift+/",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
